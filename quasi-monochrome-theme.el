(deftheme quasi-monochrome
  "Created 2018-06-01.")

(custom-theme-set-faces
 'quasi-monochrome
 '(button ((t (:inherit (link)))))
 '(cursor ((t (:background "white"))))
 '(bold ((t (:inherit (default) :weight bold))))
 '(escape-glyph ((t (:foreground "light gray"))))
 '(font-lock-builtin-face ((t (:foreground "light gray"))))
 '(font-lock-comment-delimiter-face ((t (:inherit (font-lock-comment-face)))))
 '(font-lock-comment-face ((t (:background "gray20" :foreground "light slate gray"))))
 '(font-lock-constant-face ((t (:foreground "light gray" :weight bold))))
 '(font-lock-doc-face ((t (:foreground "tomato4"))))
 '(font-lock-function-name-face ((t (:foreground "gray90"))))
 '(font-lock-keyword-face ((t (:foreground "gray40" :weight bold))))
 '(font-lock-negation-char-face ((t nil)))
 '(font-lock-preprocessor-face ((t (:inherit (font-lock-builtin-face) :foreground "light slate gray"))))
 '(font-lock-regexp-grouping-backslash ((t (:inherit (bold)))))
 '(font-lock-regexp-grouping-construct ((t (:inherit (bold)))))
 '(font-lock-string-face ((t (:foreground "AntiqueWhite4"))))
 '(font-lock-type-face ((t (:foreground "gray60" :weight bold))))
 '(font-lock-variable-name-face ((t (:foreground "gray60"))))
 '(font-lock-warning-face ((t (:foreground "dark red" :weight bold))))
 '(fringe ((t (:background "gray20"))))
 '(header-line ((t (:background "gray15" :foreground "white"))))
 '(highlight ((t (:background "gray20"))))
 '(isearch ((t (:foreground "black" :background "spring green"))))
 '(isearch-fail ((t (:background "red4"))))
 '(lazy-highlight ((t (:foreground "black" :background "lime green"))))
 '(link ((t (:underline (:color foreground-color :style line) :foreground "light gray"))))
 '(link-visited ((t (:underline (:color foreground-color :style line) :foreground "light gray"))))
 '(match ((t (:background "orange" :foreground "black"))))
 '(minibuffer-prompt ((t (:weight bold :foreground "light gray"))))
 '(mode-line ((t (:box (:line-width 6 :color "gray60") :foreground "black" :background "gray60" :height 0.9))))
 '(mode-line-buffer-id ((t (:weight bold))))
 '(mode-line-emphasis ((t (:weight bold))))
 '(mode-line-highlight ((t (:box (:line-width 2 :color "light slate gray")))))
 '(mode-line-inactive ((t (:box (:line-width 6 :color "gray20") :foreground "black" :background "gray20" :height 0.9))))
 '(next-error ((t (:inherit (region)))))
 '(query-replace ((t (:inherit (isearch)))))
 '(region ((t (:foreground "black" :background "gold"))))
 '(secondary-selection ((t (:background "gray15"))))
 '(shadow ((t (:foreground "gray60"))))
 '(tooltip ((t (:inherit (variable-pitch) :foreground "black" :background "lightyellow"))))
 '(trailing-whitespace ((t (:background "firebrick"))))
 '(default ((t (:background "gray7" :foreground "gray80"))))
 '(show-paren-match ((t (:background "gainsboro" :foreground "black" :weight bold)))))

(provide-theme 'quasi-monochrome)
